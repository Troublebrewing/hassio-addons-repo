{
  "name": "Radarr",
  "version": "latest",
  "slug": "radarr_docker",
  "description": "Radarr add-on based on the prebuilt docker image from linuxserver",
  "url": "https://github.com/marcelveldt/hassio-addons-repo/radarr",
  "startup": "services",
  "arch": [
    "aarch64",
    "amd64",
    "armhf"
  ],
  "ports": {
    "8099/tcp": 8099
  },
  "map": [
    "backup:rw",
    "share:rw",
    "ssl"
  ],
  "webui": "http://[HOST]:[PORT:8099]/",
  "boot": "auto",
  "environment": {
    "PUID": "0",
    "PGID": "0"
  },
  "options": {},
  "schema": {},
  "ingress": true,
  "panel_title": "Radarr",
  "panel_admin": true,
  "panel_icon": "mdi:movie-roll"
}